{
    "name": "ddash",
    "authors": [
        "Ali Akhtarzada"
    ],
    "description": "Utility library for D",
    "copyright": "Copyright Â© 2017, Ali Akhtarzada",
    "license": "MIT",
    "targetPath": "bin",
    "dependencies": {
        "bolts": "~>0.3.0",
        "optional": "~>0.4.0",
        "ddash:algorithm": "*",
        "ddash:functional": "*",
        "ddash:range": "*",
        "ddash:utils": "*"
    },
    "subPackages": [
        "./algorithm/",
        "./functional/",
        "./utils/",
        "./range/",
        "./common/"
    ],
    "configurations": [
        {
            "name": "unittest",
            "sourcePaths": [
                "./algorithm/",
                "./functional/",
                "./utils/",
                "./range/"
            ]
        },
        {
            "name": "benchmark",
            "targetType": "executable",
            "sourcePaths": [
                "./benchmarks/"
            ],
            "importPaths": [
                "./",
                "./benchmarks/"
            ],
            "buildOptions": [
                "optimize"
            ]
        }
    ]
}
