{
    "name": "ddash",
    "authors": [
        "Ali Akhtarzada"
    ],
    "description": "Utility library for D",
    "copyright": "Copyright Â© 2017, Ali Akhtarzada",
    "license": "MIT",
    "targetPath": "bin",
    "dependencies": {
        "ddash:algorithm": "*",
        "ddash:functional": "*",
        "ddash:range": "*",
        "ddash:utils": "*",
        "ddash:lang": "*"
    },
    "subPackages": [
        "./algorithm/",
        "./functional/",
        "./utils/",
        "./lang/",
        "./range/",
        "./common/"
    ],
    "configurations": [
        {
            "name": "unittest",
            "sourcePaths": [
                "./algorithm/",
                "./functional/",
                "./utils/",
                "./range/",
                "./lang/",
                "./common/"
            ],
            "importPaths": [
                "."
            ]
        },
        {
            "name": "unittest-safe",
            "buildType": "unittest",
            "sourcePaths": [
                "./algorithm/",
                "./functional/",
                "./utils/",
                "./range/",
                "./lang/",
                "./common/"
            ],
            "importPaths": [
                "."
            ],
            "dflags": ["-dip1000", "-dip25"]
        },
        {
            "name": "benchmark",
            "targetType": "executable",
            "sourcePaths": [
                "./benchmarks/"
            ],
            "importPaths": [
                "./",
                "./benchmarks/"
            ]
        }
    ]
}
