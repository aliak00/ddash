    "version": "2.0.0",
    "tasks": [
        {
            "label": "test",
            "type": "shell",
            "command": "dub test -- --include=testing",
            "group": {
                "kind": "test",
                "isDefault": true
            }
        },
        {
            "label": "benchmark",
            "type": "shell",
            "command": "dub run --config=benchmark --compiler=ldc2"
        }
    ]
}
